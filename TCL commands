CODE:
Create table janu;
use janu;
create table TCL(ID INT,NAME VARCHAR(30),PAYMENT INT);
SELECT * FROM TCL;
START transaction;
insert into TCL VALUES(64,'PRIYANTH',100000);
COMMIT;
START transaction;
insert into TCL VALUES(64,'PRIYANTH',0);
rollback;
START transaction;
insert into TCL VALUES(64,'PRIYANTH',1222);
savepoint A;
insert into TCL VALUES(64,'PRIYANTH',23444);
savepoint B;
insert into TCL VALUES(64,'PRIYANTH',0);
savepoint C;
rollback to B;
select * from TCL;
